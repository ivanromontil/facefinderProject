{
    "$schema": "https://github.com/Azure/static-web-apps/blob/main/docs/v1.0/schemas/config-file.schema.json",
    "routes": [
        {
            "route": "/api/*",
            "allowedRoles": ["anonymous"],
            "methods": ["GET", "POST"]
        },
        {
            "route": "/api/createEntry/index.js",
            "allowedRoles": ["anonymous"],
            "methods": ["GET", "POST"]
        }
    ],
    "navigationFallback": {
        "rewrite": "index.html",
        "exclude": ["/api/*"]
    }
}
