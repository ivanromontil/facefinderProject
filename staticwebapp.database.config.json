{
    "$schema": "https://github.com/Azure/static-web-apps/blob/main/docs/v1.0/schemas/config-file.schema.json",
    "routes": [
        {
            "route": "/api/*",
            "allowedRoles": ["anonymous"]
        }
    ],
    "navigationFallback": {
        "rewrite": "index.html",
        "exclude": ["/api/*"]
    },
    "backend": {
        "api": {
            "basePath": "api",
            "routes": [
                {
                    "route": "/createEntry",
                    "methods": ["POST"]
                }
            ]
        }
    }
}
